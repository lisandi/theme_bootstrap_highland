themes {
	content {
		# Variants for content elements
		variants {
			default {
				social = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.social
			}

			# Special variant for Header
			# Used by ctype header
			# Will be merged with default
			header {
				alert-dismissable = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.alert_dismissable
				infoboard-green = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.infoboard_green
				infoboard-blue = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.infoboard_blue
				infoboard-orange = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.infoboard_orange
				infoboard-red = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.infoboard_red
			}

			# ctype: text
			text {
				alert-dismissable = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.alert_dismissable
				infoboard-green = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.infoboard_green
				infoboard-blue = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.infoboard_blue
				infoboard-orange = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.infoboard_orange
				infoboard-red = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.infoboard_red
			}

			# ctype: gridelements_pi1
			gridelements_pi1 {
				tabs {
					navigation-pills-green = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_green
					navigation-pills-blue = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_blue
					navigation-pills-dark = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_dark
					navigation-pills-red = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_red
					navigation-pills-turquoise = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_turquoise
					navigation-pills-amethyst = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_amethyst
				}
				container {
					alert-dismissable = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.alert_dismissable
					infoboard-green = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.infoboard_green
					infoboard-blue = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.infoboard_blue
					infoboard-orange = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.infoboard_orange
					infoboard-red = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.infoboard_red
				}
				singleColumnHeaderFooter {
					panel-green = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.panel_green
					panel-blue = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.panel_blue
					panel-dark = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.panel_dark
					panel-red = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.panel_red
					panel-turquoise = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.panel_turquoise
					panel-amethyst = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.variants.panel_amethyst
				}
			}
			themes_buttoncontent_pi1 {
				button-green = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.buttons.green
				button-blue = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.buttons.blue
				button-dark = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.buttons.dark
				button-red = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.buttons.red
				button-turquoise = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.buttons.turquoise
				button-amethyst = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.buttons.amethyst
				button-large = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.buttons.large
				button-small = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.buttons.small
				button-extra-small = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.buttons.extra-small
				button-block = LLL:EXT:theme_bootstrap_highland/Resources/Private/Language/locallang.xlf:content.buttons.block
			}
		}
	}
}